#:kivy 1.0.9

<Ball>:
    size: 10, 10 
    canvas:
        Ellipse:
            pos: self.pos
            size: self.size

<MainFrame>:
    ball1: ball1
    ball2: ball2

    canvas:
        Line:
            points: self.star7_3_points
        Line:
            points: self.star12_5_points

    Ball:
        id: ball1
        speed: 12

    Ball:
        id: ball2
        speed: 3

